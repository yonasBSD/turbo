{
  "annotations": { "list": [] },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "links": [],
  "panels": [
    {
      "title": "Cache Hit Rate",
      "description": "Percentage of tasks served from cache",
      "type": "gauge",
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0,
          "max": 1,
          "color": { "mode": "thresholds" },
          "thresholds": {
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 0.5 },
              { "color": "green", "value": 0.8 }
            ]
          }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "sum(turbo_run_tasks_cached_total) / clamp_min(sum(turbo_run_tasks_attempted_total), 1)",
          "legendFormat": "",
          "refId": "A",
          "instant": true
        }
      ]
    },
    {
      "title": "Total Runs",
      "description": "Total number of turbo runs recorded",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "steps": [
              { "color": "blue", "value": null }
            ]
          }
        },
        "overrides": []
      },
      "options": { "colorMode": "value", "graphMode": "none", "textMode": "auto", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "targets": [
        {
          "expr": "sum(turbo_run_duration_ms_count)",
          "legendFormat": "",
          "refId": "A",
          "instant": true
        }
      ]
    },
    {
      "title": "Tasks Attempted",
      "description": "Total tasks attempted across all runs",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 12, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "steps": [
              { "color": "blue", "value": null }
            ]
          }
        },
        "overrides": []
      },
      "options": { "colorMode": "value", "graphMode": "none", "textMode": "auto", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "targets": [
        {
          "expr": "sum(turbo_run_tasks_attempted_total)",
          "legendFormat": "",
          "refId": "A",
          "instant": true
        }
      ]
    },
    {
      "title": "Tasks Cached",
      "description": "Tasks served from cache across all runs",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 16, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "steps": [
              { "color": "green", "value": null }
            ]
          }
        },
        "overrides": []
      },
      "options": { "colorMode": "value", "graphMode": "none", "textMode": "auto", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "targets": [
        {
          "expr": "sum(turbo_run_tasks_cached_total)",
          "legendFormat": "",
          "refId": "A",
          "instant": true
        }
      ]
    },
    {
      "title": "Tasks Failed",
      "description": "Tasks that failed across all runs",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 20, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "red", "value": 1 }
            ]
          }
        },
        "overrides": []
      },
      "options": { "colorMode": "value", "graphMode": "none", "textMode": "auto", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "targets": [
        {
          "expr": "sum(turbo_run_tasks_failed_total)",
          "legendFormat": "",
          "refId": "A",
          "instant": true
        }
      ]
    },
    {
      "title": "Run Duration Over Time",
      "description": "Average run duration across runs",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 4 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 20,
            "pointSize": 5,
            "showPoints": "always",
            "spanNulls": true
          }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "sum(turbo_run_duration_ms_sum) / sum(turbo_run_duration_ms_count)",
          "legendFormat": "avg run duration",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Task Breakdown",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 12 },
      "collapsed": false,
      "panels": []
    },
    {
      "title": "Duration by Task",
      "description": "Total execution time grouped by task name (build, lint, check-types, etc.)",
      "type": "barchart",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 13 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "color": { "mode": "palette-classic" }
        },
        "overrides": []
      },
      "options": {
        "orientation": "horizontal",
        "showValue": "always",
        "xTickLabelRotation": 0,
        "groupWidth": 0.7,
        "barWidth": 0.8
      },
      "targets": [
        {
          "expr": "sum by (turbo_task_name) (turbo_task_duration_ms_sum)",
          "legendFormat": "{{turbo_task_name}}",
          "refId": "A",
          "instant": true,
          "format": "table"
        }
      ],
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": { "Time": true },
            "renameByName": { "turbo_task_name": "Task", "Value": "Duration" }
          }
        },
        {
          "id": "sortBy",
          "options": { "fields": {}, "sort": [{ "field": "Duration", "desc": true }] }
        }
      ]
    },
    {
      "title": "Cache Status by Task",
      "description": "Cache hit vs miss count grouped by task name",
      "type": "barchart",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 13 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": { "mode": "palette-classic" }
        },
        "overrides": []
      },
      "options": {
        "orientation": "horizontal",
        "showValue": "always",
        "stacking": "normal",
        "xTickLabelRotation": 0,
        "groupWidth": 0.7,
        "barWidth": 0.8
      },
      "targets": [
        {
          "expr": "sum by (turbo_task_name, turbo_task_cache_status) (turbo_task_cache_events_total)",
          "legendFormat": "{{turbo_task_cache_status}}",
          "refId": "A",
          "instant": true,
          "format": "table"
        }
      ],
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": { "Time": true },
            "renameByName": {
              "turbo_task_name": "Task",
              "turbo_task_cache_status": "Status",
              "Value": "Count"
            }
          }
        }
      ]
    },
    {
      "title": "Task Details",
      "description": "Individual task executions with duration, cache status, and package",
      "type": "table",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 21 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {},
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Duration (ms)" },
            "properties": [{ "id": "unit", "value": "ms" }]
          }
        ]
      },
      "targets": [
        {
          "expr": "turbo_task_duration_ms_sum",
          "legendFormat": "",
          "refId": "A",
          "instant": true,
          "format": "table"
        }
      ],
      "transformations": [
        {
          "id": "filterFieldsByName",
          "options": {
            "include": {
              "names": ["turbo_task_name", "turbo_task_package", "turbo_task_cache_status", "Value"]
            }
          }
        },
        {
          "id": "organize",
          "options": {
            "renameByName": {
              "turbo_task_name": "Task",
              "turbo_task_package": "Package",
              "turbo_task_cache_status": "Cache Status",
              "Value": "Duration (ms)"
            },
            "indexByName": {
              "turbo_task_name": 0,
              "turbo_task_package": 1,
              "turbo_task_cache_status": 2,
              "Value": 3
            }
          }
        },
        {
          "id": "sortBy",
          "options": { "fields": {}, "sort": [{ "field": "Duration (ms)", "desc": true }] }
        }
      ]
    },
    {
      "title": "Task Duration Over Time",
      "description": "How task durations change across runs, grouped by task name",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 29 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10,
            "pointSize": 5,
            "showPoints": "always",
            "spanNulls": true
          }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "sum by (turbo_task_name) (turbo_task_duration_ms_sum)",
          "legendFormat": "{{turbo_task_name}}",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Cache Events Over Time",
      "description": "Cache hit/miss events per task over time",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 29 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "bars",
            "fillOpacity": 80,
            "stacking": { "mode": "normal" },
            "showPoints": "never",
            "spanNulls": true
          }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "sum by (turbo_task_name, turbo_task_cache_status) (turbo_task_cache_events_total)",
          "legendFormat": "{{turbo_task_name}} ({{turbo_task_cache_status}})",
          "refId": "A"
        }
      ]
    }
  ],
  "schemaVersion": 39,
  "tags": ["turborepo", "otel"],
  "templating": { "list": [] },
  "time": { "from": "now-1h", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "Turborepo Runs",
  "uid": "turborepo-runs",
  "version": 2
}
